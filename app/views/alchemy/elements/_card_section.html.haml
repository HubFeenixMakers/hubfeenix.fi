- cache(card_section) do
  = element_view_for(card_section) do |element|
    .bg-section_bg.font-futura
      .text-center
        .title.text-2xl.text-title
          = element.render :title
        .text-description.subtitle
          = element.render :subtitle
      = render("alchemy/elements/grid_type/#{element.render :grid}", card_section: card_section)
